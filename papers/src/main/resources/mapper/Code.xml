<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="cn.bdqn.dao.CodeMapper">
		<select id="getBy" resultType="Code">
			select * from t_code where
			parCode = '${parCode}' order by sequenceNum
		</select>
	</mapper>